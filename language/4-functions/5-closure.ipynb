    "def outer(multiplier):\n",
    "    in_mult = multiplier\n",
    "    def inner(num):\n",
    "        return num * in_mult\n",
    "    \n",
    "    return inner\n",
    "\n",
    "f1 = outer(5)   # f1 also remembers its CLOSURE, so also multiplier\n",
    "f2 = outer(7)\n",
    "print(f1(8))\n",
    "print(f2(8))\n"
   ]
  }
